{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/assets/icons/*.png"
        ],
        "urls": [
          "https://fonts.googleapis.com/css?family=Roboto:300,400,500",
          "https://fonts.gstatic.com/s/roboto/**"
        ]
      },
      "cacheConfig": {
        "maxAge": "1d",
        "strategy": "performance"
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      },
      "cacheConfig": {
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ]
}
